{
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true
	},
	"extends": [
		"wordpress",
		"plugin:vue/essential"
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2018
	},
	"rules": {
		"camelcase": 1,
		"indent": [
			"error",
			"tab"
		],
		"vue/html-indent": [
			"error", "tab"
		]
	}
}
